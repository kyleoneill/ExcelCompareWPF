	  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	  xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
	  xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
	  xmlns:local="clr-namespace:ExcelReaderWPF"
	  mc:Ignorable="d" 
	  d:DesignHeight="360" d:DesignWidth="600"
	  Title="ProgressBar">

    <Grid Margin="0,0,0,0">
        <ProgressBar x:Name="ProgressBarObj" Minimum="0" Maximum="100" Value="0" Margin="60,206,0,0" Height="28" VerticalAlignment="Top" HorizontalAlignment="Left" Width="469"/>
        <TextBox x:Name="ProgressTextBox" Height="20" Margin="197,59,0,0" TextWrapping="Wrap" VerticalAlignment="Top" IsReadOnly="True" HorizontalAlignment="Left" Width="188"/>
        <ProgressBar x:Name="ProgressBarSheets" Minimum="0" Maximum="100" Value="0" Margin="60,126,0,0" Height="28" VerticalAlignment="Top" HorizontalAlignment="Left" Width="469"/>
    </Grid>
</Page>
